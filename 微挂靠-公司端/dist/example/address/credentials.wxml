<!--example/address/credentials.wxml-->
<view class="page">
    <view class="weui-toptips f12 gray">可上传身份证、户口、结婚证等证件</view>
    <view class="profile_nav page__bd page__bd_spacing" style="margin-top:3em;">
        <view class="b12"><image style="width: {{imagewidth}}px; height: {{imageheight}}px;" src="{{imagefirstsrc}}" bindload="imageLoad" /></view>
        <view class="b12"><image style="width: {{imagewidth}}px; height: {{imageheight}}px;" src="{{imagefirstsrc}}" bindload="imageLoad" /></view>
    </view>
    <view class="weui-footer_fixed-bottom weui-footer">
        <navigator url="" class="weui-cell_access"><image src="../images/icon_upload.png" style="margin-right:8px;vertical-align: middle;width:20px; height: 20px;"></image>上传证件照</navigator>
    </view>
</view>