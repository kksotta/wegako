    <!--挂靠管理费、GPS管理费-->
        <view class="weui-post c">
            <view class="weui-cell">
                <image src="../images/icon_nav_gk.png" class="info_dp v_m"></image>挂靠管理费（GPS管理费）
            </view>
            <view class="weui-cell">
                <view class="p100 t12">
                    <view class="amount-input">+3200元</view>
                    <view class="green b12">缴费成功</view>
                    <navigator url="../trucks/archives" class="weui-flex t12">
                        <view class="weui-cell__hd">
                            <view class="weui-label l">车牌号</view>
                        </view>
                        <view class="weui-cell__bd weui-cell__ft_in-access">
                            <view class="gray l">沪D78903</view>
                        </view>
                    </navigator>
                    <navigator url="../address/profile" class="weui-flex">
                        <view class="weui-cell__hd">
                            <view class="weui-label l">车主</view>
                        </view>
                        <view class="weui-cell__bd weui-cell__ft_in-access">
                            <view class="gray l">郭靖</view>
                        </view>
                    </navigator>
                    <view class="weui-flex">
                        <view class="weui-cell__hd">
                            <view class="weui-label l">有效期</view>
                        </view>
                        <view class="weui-cell__bd">
                            <view class="gray l">2016-10-1 至 2017-9-30</view>
                        </view>
                    </view>
                </view>
            </view>
            <view class="weui-cell gray">
                缴费时间：2016-12-30 09:25
            </view>
        </view>
