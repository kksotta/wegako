<!--example/finance/withdraw.wxml-->
<view class="page">
    <view class="page__bd page__bd_spacing">
        <navigator class="weui-cell gray_bg" url="account">
            <view class="weui-cell__hd">
                <view class="weui-label">到账银行卡</view>
            </view>
            <view class="weui-cell__bd blue">招商银行（6767）</view>
        </navigator>
        <view style="background:#ffffff;">
            <view class="weui-cell col">
                <view class="p100 b12">提现金额</view>
                <view class="weui-flex">
                    <view class="h1 r15 t12">￥</view>
                    <view class="weui-flex__item"><input class="amount-input" placeholder="0.00"/></view>
                </view>
            </view>
            <view class="weui-cell">
                <view class="weui-cell__hd">可提现余额：234,580元， <text class="blue">全部提现</text></view>
            </view>
        </view>
        <view class="weui-cell">
        <view class="weui-flex__item c f12 gray">预计3个工作日内到账</view>
        </view>
        <navigator url="result"><button class="weui-btn" type="primary">提现</button></navigator>
    </view>
</view>