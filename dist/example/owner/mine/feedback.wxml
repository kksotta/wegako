<!--example/set/feedback.wxml-->
<view class="page">
    <view class="page__bd">
        <view class="weui-post b12">
            <view class="weui-cell">
                <view class="weui-cell__bd">
                    <textarea class="weui-textarea" placeholder="请描述您遇到的问题或您的宝贵意见与建议" style="height: 16em" />
                </view>
            </view>
        </view>
        <view class="weui-post">
            <view class="weui-cell">
                <view class="weui-cell__bd">
                    <input placeholder="请输入您的联系方式，以便我们及时回复您。" />
                </view>
            </view>
        </view>
    </view>
    <view class="weui-footer_fixed-bottom weui-footer">
        <navigator url="../invite/invite" class="weui-cell_access">提交</navigator>
    </view>
</view>